  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "scripts": {
    "build": "vite build",
    "cypress:e2e": "cypress open --config integrationFolder=cypress/features/e2e",
    "cypress:install": "cypress install",
    "cypress:run:e2e": "cypress run --browser chrome --config integrationFolder=cypress/features/e2e",
    "cypress:run:ui": "cypress run --browser chrome --config integrationFolder=cypress/features/ui",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ui": "cypress open --config integrationFolder=cypress/features/ui",
    "cypress": "cypress open",
    "deploy-storybook": "node scripts/deployStorybook.js",
    "dev-android": "npx cap open android",
    "dev-ios": "npx cap open ios",
    "dev-web-local-server": "node scripts/devWebLocalServer.js",
    "dev-web-local": "REACT_APP_WIPSTER_API=http://$(ifconfig en0 | awk '/inet / {print $2}'):44346/ PORT=44345 node scripts/reactStart.js",
    "dev-web:instrument": "HTTPS=true PORT=44345 node -r @cypress/instrument-cra scripts/reactStart.js",
    "dev-web": "HTTPS=true PORT=44345 node scripts/reactStart.js",
    "format": "npx prettier --write ./src",
    "lint": "eslint --ext .tsx,.ts src/",
    "raygun:buildSourcemaps": "zx scripts/raygunBuildSourcemaps.mjs",
    "start": "vite",
    "storybook": "start-storybook -p 6006 -s public",
    "terraform": "terraform -chdir=\"infrastructure/services/web\"",
    "test:ci": "jest --runInBand --coverage --coveragePathIgnorePatterns=__stories__",
    "test:coverage": "jest --coverage --coveragePathIgnorePatterns=__stories__",
    "test": "jest --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.tsx"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+/.module/.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "coverageReporters": [
      "lcov",
      "json"
    ]
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "reporter": "cobertura"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
